{
  "name": "ts-node-clean-app",
  "version": "0.0.1",
  "description": "Base Nodejs/TypeScript Clean Arhitecture application",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "lint": "eslint \"**/*.{js,ts}\"",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitica/ts-node-clean-app.git"
  },
  "keywords": [
    "clean-app",
    "ts-node-clean-app"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mitica/ts-node-clean-app/issues"
  },
  "homepage": "https://github.com/mitica/ts-node-clean-app#readme",
  "devDependencies": {
    "@types/node": "^12.0.2",
    "@types/yup": "^0.26.14",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^1.17.1",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "yup": "^0.27.0"
  }
